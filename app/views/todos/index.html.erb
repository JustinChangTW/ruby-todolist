<ul>
    <% @todos.each do |todo| %>
    <li>
        <input type="checkbox" name="todo_doen" value=<%=todo.id%> <%= todo.doen.to_s=="true" ? "checked":"" %>  >
        <%= todo.name %>
        <%= todo.due_date %>
        <%= todo.note %>
        <%= link_to  "Show", todo_path(todo) %>
        <%= link_to  "Edit", edit_todo_path(todo) %>
        <%= (todo.is_due_date_over) %>
        <% if (todo.is_due_date_over) %>
        <%= link_to  "Destroy", todo_path(todo),method: :delete,data:{confirm:"Are you sure?"} %>
        <% end %>

    </li>
    <% end %>
</ul>
<%= link_to  "New Photo", new_todo_path %>